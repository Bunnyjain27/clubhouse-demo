<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkOverlay" id="main_window_overlay">
    <property name="visible">True</property>
    <property name="halign">fill</property>
    <property name="valign">fill</property>
    <child type="overlay">
      <object class="GtkEventBox" id="main_window_overlay_msg_box">
        <property name="visible_window">True</property>
        <property name="halign">fill</property>
        <property name="hexpand">True</property>
        <property name="valign">fill</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkFixed" id="main_window_message_layer">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="halign">fill</property>
            <property name="hexpand">True</property>
            <property name="valign">fill</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <style>
          <class name="clubhouse-quest-overlay"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkBox" id="main_box">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="valign">start</property>
        <property name="orientation">vertical</property>
        <property name="spacing">12</property>
        <property name="margin">12</property>
        <child>
          <object class="GtkFixed" id="main_characters_box">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="valign">start</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
